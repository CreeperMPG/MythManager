<UserControl x:Class="MythManager.Pages.UDPAttack.AttackFunctions.CommandAttack"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MythManager.Pages.UDPAttack.AttackFunctions"
             xmlns:ui="http://schemas.inkore.net/lib/ui/wpf/modern"
             xmlns:ikw="http://schemas.inkore.net/lib/ui/wpf"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <ikw:SimpleStackPanel Spacing="16">
        <TextBox
			Name="Command"
			ui:ControlHelper.Header="CMD 命令"
			ui:ControlHelper.PlaceholderText="（支持字符串格式化）运行不存在的应用程序会导致学生端崩溃" />
        <TextBox
			Name="Arguments"
			ui:ControlHelper.Header="参数"
			ui:ControlHelper.PlaceholderText="（支持字符串格式化）启动程序的命令行参数" />
    </ikw:SimpleStackPanel>
</UserControl>
