<UserControl x:Class="MythManager.Pages.UDPAttack.AttackFunctions.MessageAttackxaml"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MythManager.Pages.UDPAttack.AttackFunctions"
             xmlns:ui="http://schemas.inkore.net/lib/ui/wpf/modern"
             xmlns:ikw="http://schemas.inkore.net/lib/ui/wpf"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <ikw:SimpleStackPanel Spacing="16">
        <TextBox
			Name="Message"
			ui:ControlHelper.Header="消息"
			ui:ControlHelper.PlaceholderText="向目标学生端发送的消息" />
    </ikw:SimpleStackPanel>
</UserControl>
